public class FindMin {
    public static void main(String[] args) {
        int[] arr = {5, 12, 3, 7, 20, 1};
        int min = arr[0];

        for (int num : arr) {
            if (num < min) {
                min = num;
            }
        }

        System.out.println("Minimum element: " + min);
    }
}
