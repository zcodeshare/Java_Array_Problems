public class FindMax {
    public static void main(String[] args) {
        int[] arr = {5, 12, 3, 7, 20, 1};
        int max = arr[0];

        for (int num : arr) {
            if (num > max) {
                max = num;
            }
        }

        System.out.println("Maximum element: " + max);
    }
}
