public class DeleteAtBeginning {
    public static void main(String[] args) {

        int[] arr = {1, 2, 3, 4, 5};

        int[] result = new int[arr.length - 1];

        for (int i = 1; i < arr.length; i++) {
            result[i - 1] = arr[i];
        }

        for (int num : result) {
            System.out.print(num + " ");
        }
    }
}
