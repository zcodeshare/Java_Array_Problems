import java.util.LinkedHashSet;

public class RemoveDuplicateElmt {
    public static void main(String[] args) {
        int[] arr = {1, 2, 3, 2, 4, 1, 5, 3};

        LinkedHashSet<Integer> set = new LinkedHashSet<>();
        for (int num : arr) {
            set.add(num);
        }

        for (int num : set) {
            System.out.print(num + " ");
        }
    }
}
