import java.util.HashSet;

public class ArrayUnion {
    public static void main(String[] args) {
        int[] a = {1, 2, 3, 4};
        int[] b = {3, 4, 5, 6};

        HashSet<Integer> union = new HashSet<>();
        for (int num : a) {
            union.add(num);
        }
        for (int num : b) {
            union.add(num);
        }

        for (int num : union) {
            System.out.print(num + " ");
        }
    }
}
