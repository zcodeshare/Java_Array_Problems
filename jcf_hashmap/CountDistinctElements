import java.util.HashSet;

public class CountDistinctElements {
    public static void main(String[] args) {
        int[] arr = {1, 2, 2, 3, 4, 4, 5};

        HashSet<Integer> set = new HashSet<>();
        int count = 0;

        for (int num : arr) {
            if (!set.contains(num)) {
                set.add(num);
                count++;
            }
        }

        System.out.println(count);
    }
}
