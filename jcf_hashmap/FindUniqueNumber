import java.util.HashMap;

public class FindUniqueNumber {
    public static void main(String[] args) {
        int[] a = {1,1,3,2,4,3,3};

        HashMap<Integer, Integer> map = new HashMap<>();

        for (int n : a) {
            map.put(n, map.getOrDefault(n, 0) + 1);
        }

        for (int k : map.keySet()) {
            if (map.get(k) == 1) {
                System.out.println(k);
            }
        }
    }
}
