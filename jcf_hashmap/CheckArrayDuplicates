import java.util.HashSet;

public class CheckArrayDuplicates {
    public static void main(String[] args) {
        int[] arr = {1, 2, 3, 4, 5, 3};

        HashSet<Integer> set = new HashSet<>();
        boolean hasDuplicate = false;

        for (int num : arr) {
            if (!set.add(num)) {
                hasDuplicate = true;
                break;
            }
        }

        System.out.println(hasDuplicate);
    }
}
