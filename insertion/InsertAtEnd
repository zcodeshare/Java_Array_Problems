public class InsertAtEnd {
    public static void main(String[] args) {

        int[] arr = {1, 2, 3, 4};
        int value = 5;

        int[] result = new int[arr.length + 1];

        for (int i = 0; i < arr.length; i++) {
            result[i] = arr[i];
        }

        result[arr.length] = value;

        for (int num : result) {
            System.out.print(num + " ");
        }
    }
}
