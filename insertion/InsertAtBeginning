public class InsertAtBeginning {
    public static void main(String[] args) {

        int[] arr = {2, 3, 4, 5};
        int value = 1;

        int[] result = new int[arr.length + 1];
        result[0] = value;

        for (int i = 0; i < arr.length; i++) {
            result[i + 1] = arr[i];
        }

        for (int num : result) {
            System.out.print(num + " ");
        }
    }
}
