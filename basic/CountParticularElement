public class CountParticularElement {
    public static void main(String[] args) {
        int[] arr = {1, 3, 2, 3, 4, 1, 3, 2, 1, 1, 4, 4, 4, 4};
        int target = 4;
        int count = 0;

        for (int num : arr) {
            if (num == target) {
                count++;
            }
        }

        System.out.println(count);
    }
}
