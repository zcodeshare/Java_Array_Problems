public class ConvertIntArrIntoDigits {
    public static void main(String[] args) {

        int[] arr = {1, 2, 3, 10};
        StringBuilder sb = new StringBuilder();

        for (int n : arr) {
            sb.append(n);
        }

        int result = Integer.parseInt(sb.toString());
        System.out.println(result);
    }
}
