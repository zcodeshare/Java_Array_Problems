public class AddElement{
    
    public int sumElement(int[] arr){
        int sum =0;
        for(int i=0;i<arr.length;i++){
            sum+=arr[i];
        }
        return sum;
    }
    
    public static void main(String[] args){
            int[] arr = {1,2,3,4,5};
            AddElement ae = new AddElement();
            int result = ae.sumElement(arr);
            System.out.println("Sum of Elements : "+result);
        }
}
